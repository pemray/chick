@charset "utf-8";
html, body, view, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, address, code, del, em, img, dl, dt, dd, ol, ul, li, fieldset, form, label, table, tbody, tfoot, thead, tr, th, td{margin: 0;padding: 0;border: 0;list-style-type: none;font-family: "微软雅黑";   word-break: break-all;}
select, input, button, textarea{outline: none;font-family: "微软雅黑";background: none;}
input[type=button]{-webkit-appearance: none;}
a{text-decoration: none; color:#000;}
body{position: relative;}
.bg{ 
  /* background-image:url("/img/bg.png");  */
  background-repeat: no-repeat; background-size: cover; width: 100%; height: 100vh; min-height: 600px; }
.prid{ position: fixed; bottom:0; left:0; width:100%;z-index: 1;}
.prid image{width:100%; display: block;}
.r_but{position: fixed; right: 20px; top:40px; z-index: 2;}
.r_but view{ padding-bottom:8px;}
.r_but view image{ width:56px; }
.l_nav{position: fixed; left: 20px; top:40px; z-index: 2;}
.tx{  height:46px;border-radius: 46px;padding:5px; background-color:rgba(0,0,0,0.3); color:#fff; position: relative;padding-left:55px;padding-right: 20px;}
.tximage{ width: 46px;height:46px;  border-radius: 50%;  position: absolute; top:5px;left:5px;}
.tximg image{ border-radius: 50%; width: 40px;height:40px;border:3px solid #fff;  }
.tx_name{ font-size:16px; line-height: 25px;}
.tx_dj{ display: table; width: 100%; font-size: 12px;}
.tx_dj>view{ display: table-cell;}
.tx_jd{ width:50px;margin-bottom:2px;margin-left:5px; height:6px; border-radius: 6px; margin-top:-3px; background-color: rgba(255,255,255,0.5);}
.tx_jd view{ background: #ffd247;height:6px; border-radius: 6px;}
.lgodl{ padding-top:20px; position: relative;margin-top:10px;}
.lgodl image{ width:50px; position: absolute; top:10px; left:0;}
.textgodl{margin-left:20px;background: #6b1fc8; color:#fff; display: inline-block; line-height:25px; border:2px solid #fff;padding-left:32px; border-radius:25px;padding-right: 10px;}
.legg{ padding-top:20px; position: relative;margin-top:20px;}
.legg image{ width:50px; position: absolute; top:10px; left:0;}
.legga{margin-left:20px;background: #ea6b3b; color:#fff; display: inline-block; line-height:25px; border:2px solid #fff;padding-left:32px; border-radius:25px;padding-right: 10px;}
.blbut{position: fixed;left:20px; bottom:20px; z-index: 2; }
.blbut image{width: 45px;}
.brbut{position: fixed;right:20px; bottom:20px; z-index: 2;}
.brbut image{width: 80px;}
.bslt{position: fixed;right:0px; bottom:90px; z-index: 2;}
.bslt image{width: 80px;}
.egg{position: fixed;right:40px; bottom:160px; z-index: 2;}
.eggg{width:150px;}
.eggcd{ position: absolute; bottom:-10px; left:5px;}
.eggcd image{ width:60px;}
.eggd{position: fixed;left:40px; bottom:160px; z-index: 3;}
.egggd{width:122px;}
.eggyy{position: absolute;left:50%;margin-left:-80px; bottom:-15px;width:160px;  z-index:-1;}
.eggyy image{width:160px;}
.egggd{width:135px;}
.num{ position: absolute; left:0; top:0; height: 30px; width:120px; text-align: center; color:#ee2e2d; font-weight: bold; font-size: 16px;padding-top:40px;}
.tage{ position: absolute;padding:5px; border-radius: 30px; background: #fbee3f; background-image: linear-gradient(#fbee3f, #c55808); left:0; top:80px; height: 15px; width:120px; text-align: center;color:#fff; font-weight: bold; font-size: 12px; text-shadow: 0px 0px 5px red; }
.tage_b{background: #fff; border-radius: 30px; height:15px; line-height: 15px; position: relative; box-shadow: 0px 5px 5px #e3bfc5 inset;}
.tage_z{ position: absolute;  width: 100%; left:0; top:0; text-align: center;z-index:1;}
.tage_red{ height: 15px; background: #eb0000;background-image: linear-gradient(#fb0000, #cc0000); border-radius: 20px;}
.char{ position:absolute; right:0; top:0; padding-top:30px; text-align: center; z-index: 3;}
.char image{width: 80px;}
.egg_time{padding-bottom:10px;padding:3px; background: #fff; border:3px solid #950d06; border-radius: 25px; padding-left:32px; position: absolute;top:-35px;left:-10%;}
.egg_time view{ font-size: 12px;  line-height: 25px; padding:0 10px; background: #ff3d3d; border-radius: 25px; color:#fff;white-space:nowrap; }
.egg_time .egg_ico{ position: absolute; width:40px; top:-10px; left:-10px;}